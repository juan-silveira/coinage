{
  "info": {
    "name": "ðŸ‘¥ Gerenciamento de UsuÃ¡rios",
    "description": "Endpoints para gerenciamento completo de usuÃ¡rios da API Azore Blockchain",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.0.0"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{access_token}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "base_url",
      "value": "{{base_url}}",
      "type": "string"
    },
    {
      "key": "api_key",
      "value": "{{api_key}}",
      "type": "string"
    },
    {
      "key": "access_token",
      "value": "{{access_token}}",
      "type": "string"
    },
    {
      "key": "user_id",
      "value": "{{user_id}}",
      "type": "string"
    },
    {
      "key": "company_id",
      "value": "{{company_id}}",
      "type": "string"
    },
    {
      "key": "email",
      "value": "{{email}}",
      "type": "string"
    },
    {
      "key": "cpf",
      "value": "{{cpf}}",
      "type": "string"
    },
    {
      "key": "wallet_address",
      "value": "{{wallet_address}}",
      "type": "string"
    },
    {
      "key": "user_name",
      "value": "{{user_name}}",
      "type": "string"
    },
    {
      "key": "password",
      "value": "{{password}}",
      "type": "string"
    },
    {
      "key": "network",
      "value": "{{network}}",
      "type": "string"
    },
    {
      "key": "page",
      "value": "{{page}}",
      "type": "string"
    },
    {
      "key": "limit",
      "value": "{{limit}}",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Create User",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "X-API-Key",
            "value": "{{api_key}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"{{user_name}}\",\n  \"email\": \"{{email}}\",\n  \"password\": \"{{password}}\",\n  \"cpf\": \"{{cpf}}\",\n  \"companyId\": \"{{company_id}}\"\n}"
        },
        "url": {
          "raw": "{{base_url}}/api/users",
          "host": ["{{base_url}}"],
          "path": ["api", "users"]
        }
      },
      "response": []
    },
    {
      "name": "List Users",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "X-API-Key",
            "value": "{{api_key}}"
          }
        ],
        "url": {
          "raw": "{{base_url}}/api/users?page={{page}}&limit={{limit}}",
          "host": ["{{base_url}}"],
          "path": ["api", "users"],
          "query": [
            {
              "key": "page",
              "value": "{{page}}"
            },
            {
              "key": "limit",
              "value": "{{limit}}"
            }
          ]
        }
      },
      "response": []
    },
    {
      "name": "Get User by ID",
      "request": {
        "method": "GET",
